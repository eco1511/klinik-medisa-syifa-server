<table id="example2" class="table table-bordered table-hover">
      <thead>
      <tr>
        <th>No.</th>
        <th>Nama</th>
        <th>Aksi</th>
      </tr>
      </thead>
      <tbody>
        <% for(let i = 0 ; i < layanan.length; i++) { %>
          <tr>
            <td><%= i + 1 %></td>
            <td><%= layanan[i].name %></td>
            <td>
              <form action="/layanan/delete/<%= layanan[i]._id %>?_method=DELETE" method="POST">
                <a href="/layanan/edit/<%= layanan[i]._id %>" class="btn btn-warning btn-xs">
                  <i class="fas fa-pencil-alt"></i>
                  Ubah</a>
                <button class="btn btn-danger btn-xs">
                  <i class="fas fa-trash"></i>
                  Hapus</button>
              </form>
              
            </td>
          </tr>
        <% } %>
      </tbody>
    </table>