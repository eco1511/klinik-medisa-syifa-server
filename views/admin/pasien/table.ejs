<table id="example2" class="table table-bordered table-hover">
      <thead>
      <tr>
        <th>No.</th>
        <th>ID</th>
        <th>Nama</th>
        <th>Tgl.Lahir</th>
        <th>Umur</th>
        <th>Jenis Kelamin</th>
        <th>Alamat</th>
        <th>Aksi</th>
      </tr>
      </thead>
      <tbody>
        <% for(let i = 0 ; i < pasien.length; i++) { %>
          <tr>
            <td><%= i + 1 %></td>
            <td><%= pasien[i].idPasien %></td>
            <td><%= pasien[i].name %></td>
            <td><%= moment(pasien[i].tgllahir).format('DD MMMM YYYY') %></td>
            <td><%= moment(pasien[i].tgllahir, 'DD MMMM YYYY').fromNow() %></td>
            <td><%= pasien[i].jenis %></td>
            <td><%= pasien[i].alamat %></td>
            <td>
              <form action="/pasien/delete/<%= pasien[i]._id %>?_method=DELETE" method="POST">
                <a href="/pasien/edit/<%= pasien[i]._id %>" class="btn btn-warning btn-xs">
                  <i class="fas fa-pencil-alt"></i>
                  Ubah</a>
                <button class="btn btn-danger btn-xs">
                  <i class="fas fa-trash"></i>
                  Hapus</button>
              </form>
              
            </td>
          </tr>
        <% } %>
      </tbody>
    </table>